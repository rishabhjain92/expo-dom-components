{
  "name": "my-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "NODE_ENV=stage npx expo start --clear",
    "start:prod": "NODE_ENV=prod npx expo start --dev-client --no-dev --minify",
    "prebuild": "yarn prebuildi && yarn prebuilda",
    "prebuilda": "expo prebuild --clean --platform android",
    "prebuildi": "expo prebuild --clean --platform ios",
    "android-release": "cd android && ./gradlew app:assembleRelease && cd .. && open android/app/build/outputs/apk/"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "expo": "~52.0.33",
    "expo-dev-client": "~5.0.12",
    "expo-status-bar": "~2.0.1",
    "highcharts": "^12.1.2",
    "highcharts-react-official": "^3.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "private": true
}
